import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num = s.nextInt();
		if ((num > 99)&&(num <= 999)) {
		  int temp = (num /10);
		  temp = temp%10;
		  if (temp % 3 == 0){
		      System.out.println("Trendy Number");
		  }
		  else {
		      System.out.println("Not Trendy Number");   
		  }
		} 
		else {
		     System.out.println("Not valid number");
		}
	}
}
