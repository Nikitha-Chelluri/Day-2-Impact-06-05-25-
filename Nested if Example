import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num = s.nextInt();
		if (num % 2 == 0) {
		    if ((num >= 2) && (num <= 5)) {
		        System.out.println("Not Weird");
		    }
		    else if ((num >= 6) &&(num <= 20)){
		        System.out.println("Weird");   
		    }
		    else if (num > 20) {
		        System.out.println("Not Weird");
		    }
		}
		else {
		    System.out.println("Weird");
		}
	}
}
